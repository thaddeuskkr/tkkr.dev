---
const site = "https://www.tkkr.dev";
const name = "thaddeus kuah";
const description = "an 19 year old hobbyist programmer and gamer who studies at nanyang polytechnic in singapore.";
const keywords = "tk,tkkr,thaddeuskkr,thaddeus,kuah";
const color = "#09090b";
const ogimage = "https://www.tkkr.dev/ogimage.jpg";
const at = "@thaddeuskkr";
---

<head>
    <meta name="application-name" content={name} />
    <meta name="description" content={description} />
    <meta name="keywords" content={keywords} />
    <meta name="author" content={name} />
    <meta name="theme-color" content={color} />
    <meta property="og:title" content={name} />
    <meta property="og:description" content={description} />
    <meta property="og:image" content={ogimage} />
    <meta property="og:url" content={site} />
    <meta property="og:type" content="website" />
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:site" content={at} />
    <meta name="twitter:title" content={name} />
    <meta name="twitter:description" content={description} />
    <meta name="twitter:image:src" content={ogimage} />
    <meta name="apple-mobile-web-app-title" content="tkkr.dev" />
</head>
